{
  "ci": {
    "collect": {
      "startServerCommand": "npm run qa:pwa:build && npm run qa:pwa:preview",
      "startServerReadyPattern": "http://127.0.0.1:4173/",
      "url": [
        "http://127.0.0.1:4173/"
      ],
      "numberOfRuns": 1,
      "settings": {
        "preset": "desktop",
        "chromeFlags": "--headless=new --no-sandbox --disable-dev-shm-usage --disable-gpu"
      }
    },
    "assert": {
      "assertions": {
        "content-width": "off"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
